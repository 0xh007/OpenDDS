{
  "name": "Continuous Integration Discovery Test",
  "desc": "This is a small quick test for (quiet) discovery capability via the relay with many participant processes",
  "any_node": [
    {
      "executable": "timed_command.pl",
      "command" : "%executable_full_path% 20 RtpsRelay -Id relay1 -UserData relay1 -DCPSDebugLevel 1 -DCPSSecurityDebugLevel 2 -LogDiscovery 1 -LogActivity 1 -ThreadMonitorPeriod 1 -LogRelayStatistics 3 -LogParticipantStatistics 1 -DCPSConfigFile %bench_root%%ds%example%ds%config%ds%relay%ds%relay1.ini -ApplicationDomain 7 -VerticalAddress 4444 -ORBLogFile %log% -DCPSPendingTimeout 3",
      "no_report": true,
      "count": 1
    },
    {
      "config": "ci_disco_relay.json",
      "count": 2
    }
  ],
  "timeout": 120
}
