project: dcpslib {
  idlflags      += -Wb,stub_export_include=Bench_Builder_Export.h \
                   -Wb,stub_export_macro=Bench_Builder_Export \
                   -I$(DDS_ROOT)/dds
  dcps_ts_flags += -Wb,export_macro=Bench_Builder_Export

  dynamicflags  += BENCH_BUILDER_BUILD_DLL
  includes += $(DDS_ROOT)/dds

  requires += no_opendds_safety_profile

  Idl_Files {
    Builder.idl
  }

  TypeSupport_Files {
    Builder.idl
  }
}

