feature(no_opendds_safety_profile) : dcps_ts_defaults {
  dcps_ts_flags += -Wb,export_macro=OpenDDS_Rtps_Export -Wb,export_include=rtps_export.h -SI -Lspcpp

  TypeSupport_Files {
    RtpsCore.idl
  }
}

feature(!no_opendds_safety_profile) : dcps_ts_defaults {
  dcps_ts_flags += -Wb,export_macro=OpenDDS_Rtps_Export -Wb,export_include=rtps_export.h -SI -Lspcpp

  TypeSupport_Files {
    RtpsCore.idl
  }
}
