project(OpenDDSUtil): dds_taolib, install {
  sharedname = OpenDDS_Util
  dynamicflags = OPENDDS_DCPS_BUILD_DLL
  includes += $(DDS_ROOT) $(DDS_ROOT)/dds
  libout = $(DDS_ROOT)/lib
  pch_header   = DCPS/DdsDcps_pch.h
  pch_source   = DCPS/DdsDcps_pch.cpp

  Source_Files {
    Hash.h
    SafetyProfileStreams.h
    Serializer.h
    XTypes
    debug.h
  }

  Header_Files {
    Hash.cpp
    SafetyProfileStreams.cpp
    Serializer.cpp
    XTypes
    debug.cpp
  }

}
