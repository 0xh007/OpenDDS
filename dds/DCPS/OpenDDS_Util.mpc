project: acelib, install {
  sharedname = OpenDDS_Util
  dynamicflags = OPENDDS_DCPS_BUILD_DLL
  includes += $(DDS_ROOT) $(DDS_ROOT)/dds
  libout = $(DDS_ROOT)/lib
  macros      += NOTAO

  Header_Files {
    Hash.h
    SafetyProfileStreams.h
    Serializer.h
    debug.h
    XTypes
  }

  Source_Files {
    Hash.cpp
    SafetyProfileStreams.cpp
    Serializer.cpp
    debug.cpp
    XTypes
  }

}
