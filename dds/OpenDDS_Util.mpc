project: acelib, install {
  sharedname = OpenDDS_Util
  dynamicflags = OPENDDS_DCPS_BUILD_DLL
  includes += $(DDS_ROOT) $(DDS_ROOT)/dds
  libout = $(DDS_ROOT)/lib

  Header_Files {
    DCPS/Hash.h
    DCPS/SafetyProfileStreams.h
    DCPS/Serializer.h
    DCPS/debug.h
    DCPS/XTypes
  }

  Source_Files {
    DCPS/Hash.cpp
    DCPS/SafetyProfileStreams.cpp
    DCPS/Serializer.cpp
    DCPS/debug.cpp
    DCPS/XTypes
  }

}
