module Unions {
  struct dummy {
    long foo;
  };

  union Z switch(boolean) {
    case TRUE: short s;
  };

  union Y switch(short) {
    case 0: string q;
  };

  union X switch(long) {
    case 78: short s;
    case 0: Z zz;
  };

  union V switch(short) {
    case 0: dummy d;
  };

  typedef sequence<dummy> dummy_sequence;
  union U switch(unsigned short) {
    case 0: dummy_sequence dsequence;
  };

  typedef dummy dummy_array[10];
  union T switch(unsigned short) {
    case 0: dummy_array darray;
  };
};

